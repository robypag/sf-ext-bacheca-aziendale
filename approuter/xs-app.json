{
    "authenticationMethod": "route",
    "welcomeFile": "index.html",
    "routes": [{
        "source": "^/admin/(.*)$",
        "target": "/admin/$1",
        "destination": "srv_api",
        "authenticationType": "xsuaa"
    }, {
        "source": "^/browse/(.*)$",
        "target": "/browse/$1",
        "destination": "srv_api",
        "authenticationType": "xsuaa"
    }, {
        "source": "^(.*)$",
        "target": "$1",
        "service": "html5-apps-repo-rt",
        "authenticationType": "xsuaa"
    }]
}